package chap03;
/**
 * 构造方法
 * @author MXJ
 *
 */
public class FangFa05 {
	private String name;
	private int age;
	FangFa05(String name2,int age2){
		name=name2;
		age=age2;
	}
	public void say(){
		System.out.println("我叫"+name+"我今年"+age+"岁了");
	}
	public static void main(String[] args) {
		FangFa05 people =new FangFa05("张三",15);
		people.say();
	}

}
